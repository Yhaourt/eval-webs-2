meta {
  name: get
  type: graphql
  seq: 2
}

post {
  url: {{GRAPHQL_URL}}
  body: graphql
  auth: none
}

body:graphql {
  query {
    reservation(id: "d638f628-d1db-4e58-b38a-38cb531212b2"){
     id
      start_time
      end_time
      status
      created_at
      room {
        id
        name
        capacity
        location
        created_at
      }
      user {
        id
        keycloak_id
        email
        created_at
      }
    }
  }
}
