meta {
  name: list
  type: graphql
  seq: 1
}

post {
  url: {{GRAPHQL_URL}}
  body: graphql
  auth: none
}

body:graphql {
  query {
    listReservations(skip: 0, limit: 10) {
      id
      start_time
      end_time
      status
      created_at
      room {
        id
        name
        capacity
        location
        created_at
      }
      user {
        id
        keycloak_id
        email
        created_at
      }
    }
  }
}
