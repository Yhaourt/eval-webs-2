meta {
  name: create
  type: graphql
  seq: 3
}

post {
  url: {{GRAPHQL_URL}}
  body: graphql
  auth: none
}

body:graphql {
  mutation {
    createUser(input: {
      keycloak_id: "1",
      email: "1@example.com"
    }) {
      id
      keycloak_id
      email
      created_at
      reservations {
        id
        start_time
        end_time
        status
        created_at
        room_id
      }
    }
  }
}
