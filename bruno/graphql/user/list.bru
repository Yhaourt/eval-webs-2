meta {
  name: list
  type: graphql
  seq: 1
}

post {
  url: {{GRAPHQL_URL}}
  body: graphql
  auth: none
}

body:graphql {
  query {
    listUsers(skip: 0, limit: 10) {
      id
      keycloak_id
      email
      created_at
      reservations {
        id
        start_time
        end_time
        status
        created_at
        room_id
      }
    }
  }
}
