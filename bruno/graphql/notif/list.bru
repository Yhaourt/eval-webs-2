meta {
  name: list
  type: graphql
  seq: 1
}

post {
  url: {{GRAPHQL_URL}}
  body: graphql
  auth: none
}

body:graphql {
  query {
    listNotifs(skip: 0, limit: 10) {
      id
      message
      notification_date
      is_sent
      reservation {
        id
        start_time
        end_time
        status
        created_at
        roomId
        userId
      }
    }
  }
}
